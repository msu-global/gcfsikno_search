name = GCFSI Knowledge Platform Search
description = A Drupal Features module that contains configuration and custom code for the search functionality.
core = 7.x
package = GCFSI Knowledge Platform
version = 7.x-1.0
project = gcfsikno_search
dependencies[] = ctools
dependencies[] = entity
dependencies[] = facetapi
dependencies[] = page_manager
dependencies[] = search_api
dependencies[] = search_api_db
dependencies[] = strongarm
dependencies[] = views
dependencies[] = views_content
features[ctools][] = facetapi:facetapi_defaults:1
features[ctools][] = page_manager:pages_default:1
features[ctools][] = strongarm:strongarm:1
features[ctools][] = views:views_default:3.0
features[facetapi][] = search_api@default_node_index:block:author
features[facetapi][] = search_api@default_node_index:block:type
features[features_api][] = api:2
features[page_manager_pages][] = search
features[search_api_index][] = default_node_index
features[search_api_server][] = local_database
features[variable][] = chosen_disable_search
features[variable][] = chosen_disable_search_threshold
features[variable][] = chosen_jquery_selector
features[variable][] = chosen_minimum_multiple
features[variable][] = chosen_minimum_single
features[variable][] = chosen_minimum_width
features[variable][] = chosen_no_results_text
features[variable][] = chosen_placeholder_text_multiple
features[variable][] = chosen_placeholder_text_single
features[variable][] = chosen_search_contains
features[variable][] = chosen_use_theme
features[views_view][] = search
